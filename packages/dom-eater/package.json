{
	"author": "artxe2",
	"bugs": {
		"url": "https://github.com/Artxe2/lube-series/issues"
	},
	"description": "AST parser for HTML that quotes JS statements using curly braces",
	"devDependencies": {
		"rollup": "^4.1.4",
		"typescript": "^5.2.2"
	},
	"exports": {
		".": {
			"import": "./src/index.js",
			"require": "./dom-eater.cjs",
			"types": "./types/index.d.ts"
		},
		"./@types": "./public.d.ts"
	},
	"keywords": ["dom-eater", "text", "dom", "ast", "eat"],
	"license": "MIT",
	"main": "src/index.js",
	"name": "dom-eater",
	"repository": {
		"directory": "packages/dom-eater",
		"type": "git",
		"url": "https://github.com/Artxe2/lube-series"
	},
	"scripts": {
		"cjs": "rollup src/index.js --file dom-eater.cjs --format cjs",
		"clean:lin": "node -e \"if (process.platform !== 'win32') process.exit(1)\" || rm -rf types",
		"clean:win": "node -e \"if (process.platform === 'win32') process.exit(1)\" || , if exist types rmdir /Q /S types",
		"dts": "npm run clean:win && npm run clean:lin && tsc",
		"test": "cd ../../dom-eater/test && vitest run"
	},
	"type": "module",
	"types": "types/index.d.ts",
	"typesVersions": {
		"*": {
			"@types": ["public.d.ts"],
			"*": ["types/index.d.ts"]
		}
	},
	"version": "0.2.3"
}