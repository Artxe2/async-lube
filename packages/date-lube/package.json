{
	"author": "artxe2",
	"bugs": {
		"url": "https://github.com/Artxe2/lube-series/issues"
	},
	"description": "Simplify Date Manipulation in JavaScript",
	"devDependencies": {
		"rollup": "^4.1.5",
		"typescript": "^5.2.2"
	},
	"exports": {
		".": {
			"import": "./src/index.js",
			"require": "./date-lube.cjs",
			"types": "./types/index.d.ts"
		},
		"./@types": "./public.d.ts"
	},
	"keywords": [
		"date",
		"lube",
		"date-lube",
		"syntax sugar"
	],
	"license": "MIT",
	"main": "src/index.js",
	"name": "date-lube",
	"repository": {
		"directory": "packages/date-lube",
		"type": "git",
		"url": "https://github.com/Artxe2/lube-series"
	},
	"scripts": {
		"cjs": "rollup src/index.js --file date-lube.cjs --format cjs",
		"clean:lin": "node -e \"if (process.platform !== 'win32') process.exit(1)\" || rm -rf types",
		"clean:win": "node -e \"if (process.platform === 'win32') process.exit(1)\" || , if exist types rmdir /Q /S types",
		"dts": "npm run clean:win && npm run clean:lin && tsc",
		"test": "cd ../../test/date-lube/ && vitest run"
	},
	"type": "module",
	"types": "types/index.d.ts",
	"typesVersions": {
		"*": {
			"@types": [
				"public.d.ts"
			],
			"*": [
				"types/index.d.ts"
			]
		}
	},
	"version": "1.4.2"
}